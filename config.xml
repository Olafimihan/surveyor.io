<?xml version='1.0' encoding='utf-8'?>
<widget id="socket.io.example" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:cdv="http://cordova.apache.org/ns/1.0"> 

    <name>surveyPlanet</name>
    
    <description>
        A Cordova application that helps auditors pick data remotely.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        DELAF SYSTEMS COMPANY
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />

    <!-- <preference name="SplashScreen" value="images/browser/splashscreen.jpg" />  -->
    <!-- <plugin name="cordova-plugin-geolocation" > -->
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />

    <platform name="android">
        <allow-intent href="market:*" />
        <icon background="icon.png" density="ldpi" foreground="icon.png" />
        
        <splash src="giphyBA.gif" density="land-hdpi"/>
    </platform>

    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    
</widget>
